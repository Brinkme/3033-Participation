<Window x:Class="GameofThornes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameofThornes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cmbBox" HorizontalAlignment="Left" Margin="277,258,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cmbBox_SelectionChanged"/>
        <Button x:Name="btnClick" Content="Button" HorizontalAlignment="Right" Margin="0,158,360,0" VerticalAlignment="Top" Click="btnClick_Click"/>
        <ListBox x:Name="lstBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="612,93,0,0"/>

    </Grid>
</Window>
