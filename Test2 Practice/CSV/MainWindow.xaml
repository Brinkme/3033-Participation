<Window x:Class="CSV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,143,0,0"/>
        <ComboBox x:Name="cmbBox" HorizontalAlignment="Left" Margin="312,58,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cmbBox_SelectionChanged"/>

    </Grid>
</Window>
